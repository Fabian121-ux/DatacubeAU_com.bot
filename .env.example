NODE_ENV=production

# WhatsApp
WA_SESSION_PATH=./session
# Set this if admins enter local numbers like 090... (example: 234)
WA_DEFAULT_COUNTRY_CODE=
WA_DEFAULT_NUMBER_ID=

# OpenRouter
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_DEFAULT_MODEL=anthropic/claude-3-haiku
OPENROUTER_MODEL=anthropic/claude-3-haiku
OPENROUTER_FALLBACK_MODEL=openai/gpt-4o-mini
OPENROUTER_MAX_TOKENS=600
OPENROUTER_TIMEOUT_MS=30000
OPENROUTER_BUDGET_DAILY_USD=2.00
OPENROUTER_RETRY_ONCE=true
OPENROUTER_CIRCUIT_FAILURE_THRESHOLD=5
OPENROUTER_CIRCUIT_COOLDOWN_MS=90000

# API
API_PORT=3001
API_EMBED_WA_CLIENT=true
API_SECRET_KEY=your-secret-key-here
ADMIN_TOKEN=your-admin-bearer-token-here
ADMIN_API_TOKEN=your-admin-bearer-token-here

# Admin Panel
ADMIN_PORT=3000
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=https://bot.datacube.au
NEXT_PUBLIC_API_URL=http://localhost:3001
ADMIN_API_BASE_URL=http://localhost:3001
ADMIN_PROXY_TIMEOUT_MS=15000
ADMIN_LOGIN_USERNAME=croneX11!
ADMIN_LOGIN_PASSWORD=factzina11!
ADMIN_SESSION_SECRET=change-this-session-secret
ADMIN_LOGIN_MAX_ATTEMPTS=5
ADMIN_LOGIN_WINDOW_MS=900000
ADMIN_LOGIN_BLOCK_MS=1800000

# Rate Limits
RATE_LIMIT_AI_PER_HOUR=5
RATE_LIMIT_MSG_PER_DAY=50
RATE_LIMIT_GLOBAL_AI_PER_MINUTE=20
AI_MODE=auto_detect
AI_RATE_LIMIT_USER=5
AI_RATE_LIMIT_GLOBAL=30
CACHE_TTL_DAYS=14
REPLY_STYLE=concise
ALLOW_IMAGE_ANALYSIS=false
OUTBOUND_QUEUE_DELAY_MS=2500
OUTBOUND_QUEUE_MIN_DELAY_MS=2000
OUTBOUND_QUEUE_MAX_DELAY_MS=4000
OUTBOUND_QUEUE_MAX_ATTEMPTS=5
OUTBOUND_QUEUE_SEND_TIMEOUT_MS=15000

# Database
DB_PATH=./data/datacube.db

# Logging
LOG_LEVEL=info
LOG_DIR=./logs
